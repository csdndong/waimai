@import"/zh_cjdianc/pages/sjzx/dndd/dnddlb.wxss";
.blbanner {padding: 10rpx 20rpx;}
.ddlb{ display: flex; flex-direction: row; background: #fff; padding: 30rpx 0;border-top: 1rpx solid #F0F0F0;border-bottom: 1rpx solid #F0F0F0; }
.ddlb>view{ flex: 1; display:flex; flex-direction:column; justify-content:space-between; align-items:center;padding: 0 20rpx; }
