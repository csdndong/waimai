.tjlb>view{ background: #fff;font-size: 22rpx; padding: 12rpx 20rpx; margin: 0rpx 15rpx; display:flex; border: 1rpx solid #d9d9d9;border-radius: 10rpx; flex-direction:row; justify-content:space-around; align-items:center; }
.image{ width: 30rpx; height: 30rpx;margin-right: 10rpx; }
.num{ font-size: 26rpx;margin-top: 30rpx;font-weight: bold; }
.text{ font-size: 26rpx; color: #999; }
