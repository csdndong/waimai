page {
    background: #EBEFF2;
}

.kefu {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 150rpx 70rpx 50rpx;
}

.kefu_one {
    background-color: white;
    border-radius: 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 200rpx;
    font-size: 30rpx;
    border: none;
}

.kefu_one>image {
    width: 70rpx;
    height: 70rpx;
    margin-right: 30rpx;
}

.kefu_two {
    margin-top: 150rpx;
}