page {
    background-color: #F4F4F4;
}

.container .content {
    margin: 10px;
}

.container .content .order-status {
    background-color: #fff;
    text-align: center;
    padding-top: 5px;
}

.container .content .order-status .status {
    font-size: 18px;
    font-weight: bold;
    color: #353535;
    padding: 9px 0;
}

.container .content .order-status .status .icon {
    display: inline-block;
    font-size: 16px;
    font-weight: normal;
}

.container .content .order-status .text {
    color: #8C8C8C;
    font-size: 14px;
    padding-bottom: 9px;
}

.container .content .order-status .become-time {
    color: #666;
    font-size: 16px;
}

.container .content .order-status .become-time .icon {
    font-size: 16px;
}

.container .content .order-status .become-time .time {
    display: inline-block;
    color: #358DE0;
}

.container .content .order-status .btn.more {
    background-color: #33aafc;
    color: #fff;
    border-color: #33aafc;
}

.container .content .order-status .order-btn {
    padding: 9px 0;
}

.container .content .order-status .order-btn .btn {
    margin: 0 5px;
    font-size: 14px;
    padding: 5px 0;
}

.container .content .order-status .deliveryer-container {
    padding: 10px 15px;
}

.container .content .order-status .deliveryer-container .avatar {
    float: left;
    display: inline-block;
    width: 50px;
    height: 50px;
    border-radius: 100%;
}

.container .content .order-status .deliveryer-container .avatar image {
    width: 100%;
    height: 100%;
    border-radius: 100%;
}

.container .content .order-status .deliveryer-container .deliveryer-info {
    display: inline-block;
    float: left;
    margin-left: 10px;
}

.container .content .order-status .deliveryer-container .deliveryer-info .name {
    font-size: 14px;
    text-align: left;
}

.container .content .order-status .deliveryer-container .deliveryer-info .name .mobile {
    color: #999;
    margin-top: 5px;
}

.container .content .order-status .deliveryer-container .deliveryer-info .star {
    text-align: left;
    display: inline-block;
}

.container .content .order-status .deliveryer-container .deliveryer-info .star .appr-score {
    display: inline-block;
    width: 10px;
    height: 10px;
    float: left;
    margin-right: 4px;
    background: url(http://cos.lalawaimai.com/we7_wmall/wxapp/newstars.png) no-repeat;
    background-position: 0 0;
    background-size: cover;
}

.container .content .order-status .deliveryer-container .deliveryer-info .deliveryer-divide {
    display: inline-block;
    font-size: 14px;
    color: #33aafc;
    margin-left: 5px;
    margin-top: 5px;
}

.container .content .order-status .deliveryer-container .operate {
    float: right;
    width: 110px;
    height: 100%;
    display: flex;
    align-items: center;
    flex-flow: row nowrap;
    justify-content: flex-end;
}

.container .content .order-status .deliveryer-container .operate navigator {
    float: left;
    height: 50px;
    width: 50%;
}

.container .content .order-status .deliveryer-container .operate .reward {
    display: inline-block;
    height: 50px;
    width: 50px;
}

.container .content .order-status .deliveryer-container .operate .tel {
    display: inline-block;
    height: 50px;
    width: 50px;
}

.container .content .order-status .deliveryer-container .operate .icon {
    width: 50px;
    height: 50px;
    line-height: 50px;
    text-align: center;
    color: #555;
}

.container .content .order-status .deliveryer-container .operate .reward .icon {
    font-size: 25px;
}

.container .content .order-status .deliveryer-container .operate .tel .icon {
    font-size: 20px;
}

.container .content .order-status .deliveryer-container .operate .location .icon {
    font-size: 28px;
}

.container .content .shop-content {
    margin-top: 10px;
    background-color: #fff;
    position: relative;
}

.container .content .shop-content .store-name {
    padding: 10px 15px;
}

.container .content .shop-content .store-name .avatar {
    width: 30px;
    height: 30px;
    display: inline-block;
    float: left;
}

.container .content .shop-content .store-name .avatar image {
    display: inline-block;
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.container .content .shop-content .store-name .name {
    display: inline-block;
    font-size: 14px;
    float: left;
    margin-top: 5px;
    margin-left: 5px;
    max-width: 80%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #7D7D7D;
}

.container .content .shop-content .store-name .more {
    float: right;
}

.container .content .shop-content .store-name .more .icon {
    line-height: 30px;
}

.container .content .shop-content .food-list .food-item {
    padding: 10px;
    background-color: #f8f8f8;
    overflow: hidden;
    margin-bottom: 10px;
}

.container .content .shop-content .food-list .food-item:last-child {
    margin-bottom: 0;
}

.container .content .shop-content .food-list .food-item .food-item-img {
    display: inline-block;
    float: left;
    width: 55px;
    height: 55px;
    overflow: hidden;
}

.container .content .shop-content .food-list .food-item .food-item-img image {
    width: 100%;
    height: 100%;
}

.container .content .shop-content .food-list .food-item .food-item-price {
    float: right;
    display: inline-block;
    line-height: 20px;
}

.container .content .shop-content .food-list .food-item .food-item-price .now-price {
    text-align: right;
    font-size: 15px;
    color: #333;
    font-weight: 500;
}

.container .content .shop-content .food-list .food-item .food-item-price .old-price {
    text-align: right;
    font-size: 12px;
    color: #999;
    text-decoration: line-through;
}

.container .content .shop-content .food-list .food-item .food-item-info {
    margin: 0 55px 0 65px;
}

.container .content .shop-content .food-list .food-item .food-item-info .food-item-name {
    line-height: 20px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 14px;
}

.container .content .shop-content .food-list .food-item .food-item-info .food-item-name .tags::before {
    padding: 0 3px;
}

.container .content .shop-content .food-list .food-item .food-item-info .food-item-num {
    line-height: 18px;
    color: #999;
    font-size: 12px;
}

.container .content .shop-content .order-region-entries {
    margin: 0 15px;
}

.container .content .shop-content .order-region-entries .order-region-entry {
    font-size: 14px;
    position: relative;
    color: #2f2f2f;
    overflow: hidden;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
    padding-bottom: 5px;
}

.container .content .shop-content .order-region-entries .order-region-entry:first-child {
    padding-top: 9px;
}

.container .content .shop-content .order-region-entries .order-region-entry:last-child {
    padding-bottom: 9px;
}

.container .content .shop-content .order-region-entries .order-region-entry .order-entry-price {
    font-size: 12px;
}

.container .content .shop-content .discount-box .discount {
    margin-left: 15px;
    padding: 5px 0;
    line-height: 20px;
}

.container .content .shop-content .discount-box .discount .discount-name {
    display: inline-block;
    font-size: 14px;
}

.container .content .shop-content .discount-box .discount .discount-name .tags {
    display: inline-block;
    font-size: 14px;
    text-align: center;
}

.container .content .shop-content .discount-box .discount .discount-name .tags::before {
    padding: 0 3px;
}

.container .content .shop-content .discount-box .discount .discount-price {
    float: right;
    margin-right: 15px;
    display: inline-block;
    font-size: 14px;
    color: #FF4648;
}

.container .content .shop-content .price {
    padding: 10px 15px;
}

.container .content .shop-content .price .total {
    float: right;
    display: inline-block;
    font-size: 16px;
}

.container .content .shop-content .price .total view {
    display: inline-block;
    font-size: 14px;
    font-weight: bold;
    color: #FF4648;
}

.container .content .shop-content .price .discount {
    float: right;
    display: inline-block;
    margin-right: 10px;
    font-size: 15px;
    color: #7D7D7D;
    margin-top: 2px;
}

.container .content .shop-content .order-pay-info {
    position: absolute;
    bottom: 120px;
    left: 50%;
}

.container .content .shop-content .order-pay-info.have-pay {
    background: url(http://1.duoxunwl.com/addons/we7_wmall/template/mobile/manage/static/img/have_pay.png) no-repeat 0 0;
    background-size: cover;
    width: 80px;
    height: 44px;
    margin-left: -40px;
}

.container .content .shop-content .order-pay-info.delivery-pay {
    background: url(http://1.duoxunwl.com/addons/we7_wmall/template/mobile/manage/static/img/delivery_pay.png) no-repeat 0 0;
    background-size: cover;
    width: 120px;
    height: 80px;
    margin-left: -60px;
}

.container .content .shop-content .store-tel {
    text-align: center;
    padding: 10px 15px;
    font-size: 16px;
}

.container .content .shop-content .store-tel .icon {
    font-size: 16px;
    color: #555;
}

.container .content .dispatching {
    padding: 15px 15px;
    background-color: #fff;
    margin-top: 10px;
}

.container .content .dispatching .time>view {
    float: left;
}

.container .content .dispatching .address>view {
    float: left;
}

.container .content .dispatching .server>view {
    float: left;
}

.container .content .dispatching .text {
    font-size: 14px;
    color: #9A9A9A;
    margin-right: 15px;
}

.container .content .dispatching .time .main {
    font-size: 14px;
}

.container .content .dispatching .address {
    margin-top: 15px;
}

.container .content .dispatching .address .main {
    font-size: 14px;
    margin-left: 71px;
    float: none;
}

.container .content .dispatching .server {
    margin-top: 15px;
}

.container .content .dispatching .server .main {
    font-size: 14px;
}

.container .content .dispatching .server .main view {
    display: inline-block;
    float: left;
}

.container .content .dispatching .server .main .allocation-icon {
    margin-top: 3px;
}

.container .content .dispatching .server .main view:last-child {
    margin-left: 5px;
}

.container .content .order {
    padding: 15px;
    margin-top: 10px;
    background-color: #fff;
    position: relative;
}

.container .content .order .code>view {
    display: inline-block;
    font-size: 14px;
}

.container .content .order .time>view {
    display: inline-block;
    font-size: 14px;
}

.container .content .order .pay-type>view {
    display: inline-block;
    font-size: 14px;
}

.container .content .order .time {
    margin-top: 10px;
}

.container .content .order .pay-type,.container .content .order .note {
    margin-top: 10px;
}

.container .content .order .text {
    color: #9A9A9A;
    margin-right: 15px;
    min-width: 70px;
}

.container .content .order .note .text {
    font-size: 14px;
    color: #9A9A9A;
    margin-right: 15px;
    float: left;
}

.container .content .order .note .main {
    font-size: 14px;
    margin-left: 71px;
    float: none;
}

.container .content .order .copy {
    position: absolute;
    right: 15px;
    top: 15px;
    font-size: 12px;
    border: 1px solid #B4B4B4;
    padding: 2px 10px;
    border-radius: 3px;
}

.bottom-fixed.close {
    background-color: #33aafc;
    color: #fff;
}

.pop-container .wui-steps {
    margin: 0 10px;
}

.pop-container .wui-steps-title {
    width: 90%;
}

.pop-container .time {
    display: inline-block;
    float: right;
    color: #999;
}

.pop-container .explan {
    padding-left: 27px;
    color: #999;
}

.icon-b {
    width: 17px;
    height: 17px;
}